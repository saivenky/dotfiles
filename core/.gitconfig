[core]
    pager = delta
[init]
	defaultBranch = main
[push]
	default = current
[alias]
	co = checkout
	fix = commit -a --amend --no-edit
	rebase = rebase -i HEAD~10
	r10 = rebase -i HEAD~10
	create = !git checkout main && git fetch && git checkout -b
	word-diff = diff --word-diff
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
